// Interface
interface Bank {
    double rateOfInterest();   // abstract method
}

// SBI implements Bank
class SBI implements Bank {
    @Override
    public double rateOfInterest() {
        return 6.5;   // SBI interest rate
    }
}

// HDFC implements Bank
class HDFC implements Bank {
    @Override
    public double rateOfInterest() {
        return 7.2;   // HDFC interest rate
    }
}

// Main Class
public class Main {
    public static void main(String[] args) {

        Bank b;

        b = new SBI();
        System.out.println("SBI Interest Rate: " + b.rateOfInterest() + "%");

        b = new HDFC();
        System.out.println("HDFC Interest Rate: " + b.rateOfInterest() + "%");
    }
}
