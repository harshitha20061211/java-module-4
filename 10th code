Vehicle.java

(Inside folder: vehicles/)

package vehicles;

public interface Vehicle {
    void speed();
}

Bike.java

(Inside folder: vehicles/)

package vehicles;

public class Bike implements Vehicle {
    @Override
    public void speed() {
        System.out.println("Bike speed: 80 km/h");
    }
}

 Car.java

(Inside folder: vehicles/)

package vehicles;

public class Car implements Vehicle {
    @Override
    public void speed() {
        System.out.println("Car speed: 150 km/h");
    }
}
MainTest.java

(This file is outside the vehicles package)

import vehicles.Bike;
import vehicles.Car;

public class MainTest {
    public static void main(String[] args) {

        Bike b = new Bike();
        b.speed();

        Car c = new Car();
        c.speed();
    }
}

